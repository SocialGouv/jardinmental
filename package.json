  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.13.2",
    "@react-native-community/checkbox": "^0.5.6",
    "@react-native-community/datetimepicker": "^3.0.6",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/push-notification-ios": "^1.7.4",
    "@react-navigation/material-top-tabs": "^5.3.10",
    "@react-navigation/native": "^5.8.10",
    "@react-navigation/stack": "^5.12.8",
    "add": "^2.0.6",
    "date-fns": "^2.16.1",
    "react": "16.14.0",
    "react-native": "0.63.3",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-push-notification": "^6.1.3",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.14.0",
    "react-native-svg": "^12.1.0",
    "react-native-tab-view": "^2.15.2",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@babel/core": "^7.12.8",
    "@babel/runtime": "^7.12.5",
    "@bam.tech/react-native-make": "^3.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.5.2",
    "eslint": "^7.13.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "16.14.0"
  },
  "jest": {
    "preset": "react-native",
    "moduleNameMapper": {
      "\\.svg": "<rootDir>/__mocks__/svgMock.js"
    },
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ]
  }
