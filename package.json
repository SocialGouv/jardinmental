{
  "name": "monsuivipsy",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "postinstall": "pod install --project-directory=ios/ && npx jetify",
    "lint": "eslint .",
    "build:android": "cd android && ./gradlew bundleRelease && cd ../",
